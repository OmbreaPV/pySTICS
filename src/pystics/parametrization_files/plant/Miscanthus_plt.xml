<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<fichierplt version="10.0">
  <formalisme nom="plant name and group">
    <param format="character" nom="codeplante">Mis</param>
    <option choix="1" nom="monocot or dicot" nomParam="codemonocot">
      <choix code="1" nom="monocotyledon"/>
      <choix code="2" nom="dicotyledon"/>
    </option>
  </formalisme>
  <formalisme nom="effect of atmospheric CO2 concentration">
    <param format="real" max="2.0" min="1.0" nom="alphaco2">1.1</param>
  </formalisme>
  <formalisme nom="phasic development">
    <param format="real" max="15.0" min="-10.0" nom="tdmin">6.00000</param>
    <param format="real" max="40.0" min="15.0" nom="tdmax">25.00000</param>
    <option choix="1" nom="driving temperature" nomParam="codetemp">
      <choix code="1" nom="air temperature">
        <option choix="1" nom="time.scale" nomParam="codegdh">
          <choix code="1" nom="daily.scale"/>
          <choix code="2" nom="hourly.scale"/>
        </option>
      </choix>
      <choix code="2" nom="temperature within canopy">
        <param format="real" max="2.0" min="1.0" nom="coeflevamf">-999</param>
        <param format="real" max="2.0" min="1.0" nom="coefamflax">-999</param>
        <param format="real" max="2.0" min="1.0" nom="coeflaxsen">-999</param>
        <param format="real" max="2.0" min="1.0" nom="coefsenlan">-999</param>
        <param format="real" max="2.0" min="1.0" nom="coeflevdrp">-999</param>
        <param format="real" max="2.0" min="1.0" nom="coefdrpmat">-999</param>
        <param format="real" max="2.0" min="1.0" nom="coefflodrp">-999</param>
      </choix>
    </option>
    <option choix="2" nom="photoperiodic plant" nomParam="codephot">
      <choix code="1" nom="yes">
        <option choix="2" nom="effect of decreasing photoperiod on biomass allocation" nomParam="codephot_part">
          <choix code="1" nom="yes"/>
          <choix code="2" nom="no"/>
        </option>
      </choix>
      <choix code="2" nom="no"/>
    </option>
    <option choix="1" nom="delay effect of stress" nomParam="coderetflo">
      <choix code="1" nom="yes">
        <param format="real" max="0.9" min="0.1" nom="stressdev">0.25</param>
      </choix>
      <choix code="2" nom="no"/>
    </option>
    <option choix="3" nom="cold requirements" nomParam="codebfroid">
      <choix code="1" nom="no"/>
      <choix code="2" nom="vernalisation (herbaceous)">
        <param format="integer" max="7" min="0" nom="jvcmini">-999</param>
        <param format="integer" max="731" min="1" nom="julvernal">-999</param>
        <param format="real" max="10.0" min="-5.0" nom="tfroid">-999</param>
        <param format="real" max="30.0" min="1.0" nom="ampfroid">-999</param>
      </choix>
      <choix code="3" nom="dormancy (woody)">
        <param format="real" max="40.0" min="0.0" nom="tdmindeb">0</param>
        <param format="real" max="40.0" min="0.0" nom="tdmaxdeb">50</param>
        <option choix="1" nom="dormancy calculation" nomParam="codedormance">
          <choix code="1" nom="forcing">
            <param format="integer" max="731" min="1" nom="ifindorm">15</param>
          </choix>
          <choix code="2" nom="Richardson"/>
          <choix code="3" nom="Bidabe">
            <param format="real" max="3.5" min="1.5" nom="q10">-999</param>
            <param format="integer" max="731" min="1" nom="idebdorm">-999</param>
          </choix>
        </option>
        <option choix="1" nom="post dormancy calculation" nomParam="codegdhdeb">
          <choix code="1" nom="daily temperatures">
            <option choix="1" nom=" Wang et Engel (1998)" nomParam="code_WangEngel">
              <choix code="1" nom="Wang et Engel">
                <param format="real" max="30" min="0" nom="tdoptdeb">11.7</param>
              </choix>
              <choix code="2" nom="no"/>
            </option>
          </choix>
          <choix code="2" nom="hourly temperatures"/>
        </option>
      </choix>
    </option>
  </formalisme>
  <formalisme nom="emergence and starting">
    <param format="real" max="15.0" min="-10.0" nom="tgmin">6.0</param>
    <option choix="2" nom="annual or perennial" nomParam="codeperenne">
      <choix code="1" nom="annual"/>
      <choix code="2" nom="perennial"/>
    </option>
    <option choix="2" nom="germination or latency" nomParam="codegermin">
      <choix code="1" nom="yes">
        <param format="real" max="100.0" min="0.0" nom="stpltger">-999</param>
        <param format="real" max="1.0" min="1.0E-4" nom="potgermi">-999</param>
        <param format="integer" max="50" min="1" nom="nbjgerlim">-999</param>
        <param format="real" max="100.0" min="0.0" nom="propjgermin">-999</param>
      </choix>
      <choix code="2" nom="no"/>
    </option>
    <option choix="2" nom="plantlet growth" nomParam="codehypo">
      <choix code="1" nom="hypocotyle growth">
        <param format="real" max="0.04" min="0.005" nom="belong">-999</param>
        <param format="real" max="10.0" min="1.0" nom="celong">-999</param>
        <param format="real" max="40.0" min="2.0" nom="elmax">-999</param>
        <param format="integer" max="100" min="1" nom="nlevlim1">-999</param>
        <param format="integer" max="100" min="1" nom="nlevlim2">-999</param>
        <param format="real" max="1.0" min="1.0E-4" nom="vigueurbat">-999</param>
      </choix>
      <choix code="2" nom="planting">
        <param format="real" max="8.0" min="0.0" nom="laiplantule">0</param>
        <param format="integer" max="10" min="0" nom="nbfeuilplant">0</param>
        <param format="real" max="4.0" min="0.002" nom="masecplantule">0</param>
        <param format="real" max="200.0" min="0.0" nom="zracplantule">1.0</param>
      </choix>
    </option>
  </formalisme>
  <formalisme nom="leaves">
    <param format="real" max="150.0" min="10.0" nom="phyllotherme">70</param>
    <param format="real" max="1.0" min="0.4" nom="laicomp">3</param>
    <param format="real" max="15.0" min="-10.0" nom="tcmin">6.0</param>
    <param format="real" max="50.0" min="10.0" nom="tcmax">25.0</param>
    <param format="real" max="50.0" min="1.0E-4" nom="tcxstop">30</param>
    <option choix="1" nom="leaf dynamics" nomParam="codelaitr">
      <choix code="1" nom="LAI">
        <param format="real" max="2.5" min="1.5" nom="vlaimax">1.5</param>
        <param format="real" max="10.0" min="0.0" nom="pentlaimax">5</param>
        <param format="real" max="3.0" min="1.0" nom="udlaimax">1.85</param>
        <param format="real" max="1.0" min="0.0" nom="ratiodurvieI">0.35</param>
        <param format="real" max="1.0" min="0.0" nom="ratiosen">0.5</param>
        <param format="real" max="1.0" min="0.0" nom="abscission">0.85</param>
        <param format="real" max="200.0" min="10.0" nom="parazofmorte">60</param>
        <param format="real" max="1.0" min="-2.0" nom="innturgmin">-2</param>
        <param format="real" max="1.0" min="0.01" nom="dlaimin">0.01</param>
        <option choix="2" nom="LAI calculation option" nomParam="codlainet">
          <choix code="1" nom="direct LAInet">
            <param format="real" max="1.0" min="0.3" nom="tustressmin">-999</param>
          </choix>
          <choix code="2" nom="LAInet=LAIbrut-senes">
            <param format="real" max="1.0" min="0.0" nom="durviesupmax">0</param>
          </choix>
        </option>
        <option choix="1" nom="effect of photoperiod on senescence" nomParam="codestrphot">
          <choix code="1" nom="yes">
            <param format="real" max="12.5" min="1.0" nom="phobasesen">12.15</param>
            <param format="real" max="0.1" min="0.01" nom="dltamsmaxsen">0.1</param>
            <param format="real" max="0.1" min="0.01" nom="dltamsminsen">0.01</param>
            <param format="real" max="1000.0" min="0.0" nom="alphaphot">0.25</param>
          </choix>
          <choix code="2" nom="no"/>
        </option>
      </choix>
      <choix code="2" nom="ground cover">
        <param format="real" max="1.0" min="0.5" nom="tauxrecouvmax">-999</param>
        <param format="real" max="1.0" min="0.5" nom="tauxrecouvkmax">-999</param>
        <param format="real" max="10.0" min="0.0" nom="pentrecouv">-999</param>
        <param format="real" max="3.0" min="0.0" nom="infrecouv">-999</param>
      </choix>
    </option>
  </formalisme>
  <formalisme nom="radiation interception">
    <option choix="1" nom="radiation interception" nomParam="codetransrad">
      <choix code="1" nom="Beer's law"/>
      <choix code="2" nom="radiation transfers">
        <param format="integer" max="3" min="1" nom="forme">-999</param>
        <param format="real" max="5.0" min="-5.0" nom="rapforme">-999</param>
        <param format="real" max="10.0" min="-10.0" nom="adfol">-999</param>
        <param format="real" max="10.0" min="1.0" nom="dfolbas">-999</param>
        <param format="real" max="10.0" min="1.0" nom="dfolhaut">-999</param>
      </choix>
    </option>
  </formalisme>
  <formalisme nom="shoot biomass growth">
    <param format="real" max="40.0" min="15.0" nom="temax">35</param>
    <param format="real" max="30.0" min="10.0" nom="teoptbis">30</param>
    <param format="real" max="7.0" min="1.0" nom="efcroijuv">2.6</param>
    <param format="real" max="10.0" min="1.0" nom="efcroiveg">5.2</param>
    <param format="real" max="10.0" min="1.0" nom="efcroirepro">2.6</param>
    <param format="real" max="0.5" min="0.0" nom="remobres">0.15</param>
    <param format="real" max="50.0" min="2.0" nom="coefmshaut">7.5</param>
  </formalisme>
  <formalisme nom="partitioning of biomass in organs">
    <param format="real" max="500.0" min="50.0" nom="slamin">130</param>
    <param format="real" max="0.5" min="0.0" nom="envfruit">0.1</param>
    <param format="real" max="300.0" min="10.0" nom="sea">100</param>
    <option choix="1" nom="Simulation of Nitrogen and Carbon reserves" nomParam="code_acti_reserve">
      <choix code="1" nom="yes">
        <param format="real" max="1.0" min="0.0" nom="propresP">0.45</param>
        <param format="real" max="1.0" min="0.0" nom="PropresPN">0.6</param>
        <param format="real" max="1.0" min="0.0" nom="Efremobil">0.05</param>
        <param format="real" max="1.0" min="0.0" nom="Propres">0.12</param>
        <param format="real" max="1.0" min="0.0" nom="tauxmortresP">0.0005</param>
        <param format="real" max="200.0" min="5.0" nom="Parazoper">45</param>
        <param format="real" max="200.0" min="10.0" nom="ParazoTmorte">120</param>
        <param format="real" max="0.1" min="0" nom="inilai">0.001</param>
      </choix>
      <choix code="2" nom="no">
        <param format="real" max="100.0" min="0.0" nom="resplmax">-999</param>
      </choix>
    </option>
    <option choix="2" nom="Calculation of the stem elongation stage for perenial grasslands" nomParam="codemontaison">
      <choix code="1" nom="yes"/>
      <choix code="2" nom="no"/>
    </option>
    <option choix="2" nom="Simulation of tiller dynamics for grasslands" nomParam="codedyntalle">
      <choix code="1" nom="yes">
        <option choix="1" nom="Choice of the ratio used to calculate tiller mortality" nomParam="codetranspitalle">
          <choix code="1" nom="et/etm"/>
          <choix code="2" nom="ep/eop"/>
        </option>
        <param format="real" max="0.00001" min="0.0000025" nom="SurfApex">-999</param>
        <param format="real" max="0.04" min="0.01" nom="SeuilMorTalle">-999</param>
        <param format="real" max="0.2" min="0.1" nom="SigmaDisTalle">-999</param>
        <param format="real" max="0.01" min="0.00001" nom="VitReconsPeupl">-999</param>
        <param format="real" max="1200" min="400" nom="SeuilReconsPeupl">-999</param>
        <param format="real" max="5000" min="3000" nom="MaxTalle">-999</param>
        <param format="real" max="2.0" min="0.5" nom="SeuilLAIapex">-999</param>
        <param format="real" max="10.0" min="1.0" nom="tigefeuilcoupe">-999</param>
      </choix>
      <choix code="2" nom="no"/>
    </option>
  </formalisme>
  <formalisme nom="yield formation">
    <param format="real" max="10000.0" min="0.0" nom="irazomax">0</param>
    <option choix="1" nom="growing dynamics" nomParam="codeindetermin">
      <choix code="1" nom="determinate growing plant">
        <param format="real" max="1.0" min="0.00" nom="cgrain">0</param>
        <param format="real" max="15000.0" min="0.0" nom="cgrainv0">0.000</param>
        <option choix="2" nom="unit Harvest Index" nomParam="codeir">
          <choix code="1" nom="days">
            <param format="real" max="1.0" min="0.2" nom="irmax">-999</param>
          </choix>
          <choix code="2" nom="degree days"/>
        </option>
      </choix>
      <choix code="2" nom="indeterminate growing plant">
        <param format="integer" max="20" min="1" nom="nboite">-999</param>
        <param format="real" max="0.3" min="0.05" nom="allocfrmax">-999</param>
        <param format="real" max="1.0" min="0.01" nom="afpf">-999</param>
        <param format="real" max="30.0" min="0.0" nom="bfpf">-999</param>
        <param format="real" max="100.0" min="0.1" nom="cfpf">-999</param>
        <param format="real" max="5.0" min="0.01" nom="dfpf">-999</param>
        <param format="real" max="1.0" min="0.0" nom="spfrmin">-999</param>
        <param format="real" max="2.0" min="0.7" nom="spfrmax">-999</param>
        <param format="real" max="1.0" min="0.01" nom="splaimin">-999</param>
        <param format="real" max="2.0" min="0.7" nom="splaimax">-999</param>
        <option choix="2" nom="number of inflorescences" nomParam="codcalinflo">
          <choix code="1" nom="prescribed"/>
          <choix code="2" nom="trophic status function"/>
        </option>
      </choix>
    </option>
    <option choix="2" nom="thermal stress on filling" nomParam="codetremp">
      <choix code="1" nom="yes">
        <param format="real" max="20.0" min="0.0" nom="tminremp">-999</param>
        <param format="real" max="40.0" min="10.0" nom="tmaxremp">-999</param>
      </choix>
      <choix code="2" nom="no"/>
    </option>
  </formalisme>
  <formalisme nom="roots">
    <param format="real" max="1.0" min="0.0" nom="sensanox">0</param>
    <param format="character" nom="stoprac">lax</param>
    <param format="real" max="1.0" min="0.0" nom="sensrsec">1</param>
    <param format="real" max="1.0" min="0.0" nom="contrdamax">0</param>
    <param format="real" max="0.07" min="0.005" nom="rayon">0.025</param>
    <option choix="2" nom="driving temperature" nomParam="codetemprac">
      <choix code="1" nom="temperature within the canopy"/>
      <choix code="2" nom="soil (threshold TGMIN)"/>
    </option>
    <option choix="2" nom="calculation of the root death at cutting date for grasslands" nomParam="codemortalracine">
      <choix code="1" nom="function of dry matter production between two successives cut">
        <param format="real" max="1.0" min="0.0" nom="coefracoupe">-999</param>
      </choix>
      <choix code="2" nom="no specific root death at cutting"/>
    </option>
    <option choix="2" nom="root density" nomParam="coderacine">
      <choix code="1" nom="standard profile">
        <param format="real" max="100.0" min="0.0" nom="zlabour">-999</param>
        <param format="real" max="200.0" min="10.0" nom="zpente">-999</param>
        <param format="real" max="200.0" min="10.0" nom="zprlim">-999</param>
      </choix>
      <choix code="2" nom="true density">
        <param format="real" max="1000.0" min="1.0" nom="draclong">400</param>
        <param format="real" max="3000.0" min="0.0" nom="debsenrac">2250</param>
        <param format="real" max="1.0" min="0.02" nom="lvfront">0.075</param>
        <param format="real" max="25000.0" min="250.0" nom="longsperac">1553</param>
        <option choix="1" nom="standard root distribution" nomParam="codedisrac">
          <choix code="1" nom="yes">
            <param format="real" max="0.01" min="0.00001" nom="kdisrac">0.002</param>
            <param format="real" max="1.0" min="0.0" nom="alloperirac">0</param>
          </choix>
          <choix code="2" nom="no"/>
        </option>
        <option choix="2" nom="N effect on root distribution" nomParam="codazorac">
          <choix code="1" nom="yes">
            <param format="real" max="1.0" min="1.0E-4" nom="minefnra">-999</param>
            <param format="real" max="5.0" min="1.0E-4" nom="minazorac">-999</param>
            <param format="real" max="5.0" min="1.0E-4" nom="maxazorac">-999</param>
          </choix>
          <choix code="2" nom="no"/>
        </option>
        <option choix="1" nom="trophic-linked production" nomParam="codtrophrac">
          <choix code="1" nom="continuous link">
            <param format="real" max="0.9" min="0.1" nom="repracpermax">0.10</param>
            <param format="real" max="0.9" min="0.1" nom="repracpermin">0.15</param>
            <param format="real" max="2.0" min="1.0E-4" nom="krepracperm">2</param>
          </choix>
          <choix code="2" nom="threshold">
            <param format="real" max="0.9" min="0.1" nom="repracseumax">-999</param>
            <param format="real" max="0.9" min="0.1" nom="repracseumin">-999</param>
            <param format="real" max="2.0" min="1.0E-4" nom="krepracseu">-999</param>
          </choix>
          <choix code="3" nom="no"/>
        </option>
        <option choix="1" nom="Preferential allocation of biomass to roots in case of water or N stress" nomParam="code_stress_root">
          <choix code="1" nom="yes"/>
          <choix code="2" nom="no"/>
        </option>
        <option choix="1" nom="rootdeposition activation" nomParam="code_rootdeposition">
          <choix code="1" nom="yes">
            <param format="real" max="100.0" min="20.0" nom="parazorac">30</param>
            <option choix="1" nom="simulation of 2 roots classes" nomParam="code_diff_root">
              <choix code="1" nom="yes">
                <param format="real" max="4.0" min="0.0" nom="lvmax">2.6</param>
                <param format="real" max="10.0" min="1.0" nom="rapdia">2.6</param>
                <param format="real" max="0.80" min="0.05" nom="RTD">0.3</param>
                <param format="real" max="1.0" min="0.0" nom="propracfmax">0.8</param>
              </choix>
              <choix code="2" nom="no"/>
            </option>
          </choix>
          <choix code="2" nom="no"/>
        </option>
      </choix>
    </option>
  </formalisme>
  <formalisme nom="frost">
    <param format="real" max="-1.0" min="-30.0" nom="tletale">-30.00000</param>
    <param format="real" max="5.0" min="-5.0" nom="tdebgel">-5.00000</param>
    <option choix="2" nom="plantlet or emergence frost" nomParam="codgellev">
      <choix code="1" nom="no"/>
      <choix code="2" nom="yes">
        <param format="integer" max="5" min="1" nom="nbfgellev">3</param>
        <param format="real" max="0.0" min="-25.0" nom="tgellev90">-8.0</param>
      </choix>
    </option>
    <option choix="2" nom="leaf frost at juvenile phase (till AMF)" nomParam="codgeljuv">
      <choix code="1" nom="no"/>
      <choix code="2" nom="yes">
        <param format="real" max="0.0" min="-25.0" nom="tgeljuv90">-8.00000</param>
      </choix>
    </option>
    <option choix="2" nom="leaf frost at adult phase" nomParam="codgelveg">
      <choix code="1" nom="no"/>
      <choix code="2" nom="yes">
        <param format="real" max="0.0" min="-25.0" nom="tgelveg90">-8.00000</param>
      </choix>
    </option>
    <option choix="1" nom="flower/fruit frost (from FLO)" nomParam="codgelflo">
      <choix code="1" nom="no"/>
      <choix code="2" nom="yes">
        <param format="real" max="0.0" min="-25.0" nom="tgelflo10">-999</param>
        <param format="real" max="0.0" min="-25.0" nom="tgelflo90">-999</param>
      </choix>
    </option>
  </formalisme>
  <formalisme nom="water">
    <param format="real" max="1.0" min="0.5" nom="h2ofeuilverte">0.63000</param>
    <param format="real" max="1.0" min="0.05" nom="h2ofeuiljaune">0.10000</param>
    <param format="real" max="1.0" min="0.5" nom="h2otigestruc">0.59000</param>
    <param format="real" max="1.0" min="0.5" nom="h2oreserve">0.62000</param>
    <param format="real" max="1.0" min="0.1" nom="h2ofrvert">0.40000</param>
    <param format="real" max="0.05" min="1.0E-4" nom="tempdeshyd">0.00500</param>
    <option choix="1" nom="water requirements" nomParam="codebeso">
      <choix code="1" nom="crop coefficient">
        <param format="real" max="4.0" min="1.0" nom="kmax">1.0</param>
      </choix>
      <choix code="2" nom="resistance approach">
        <param format="real" max="500.0" min="20.0" nom="rsmin">-999</param>
      </choix>
    </option>
    <option choix="2" nom="interception of water by foliage" nomParam="codeintercept">
      <choix code="1" nom="yes">
        <param format="real" max="3.0" min="0.05" nom="mouillabil">-999</param>
        <param format="real" max="1.0" min="0.0" nom="stemflowmax">-999</param>
